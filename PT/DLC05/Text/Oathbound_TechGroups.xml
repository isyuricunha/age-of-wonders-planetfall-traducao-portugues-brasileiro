   <Row ss:AutoFitHeight="0">
    <Cell><Data ss:Type="String">TACTICAL_OPERATION</Data></Cell>
    <Cell><Data ss:Type="String">TACTICAL_OP_6_NAME</Data></Cell>
    <Cell><Data ss:Type="String">Apressar o Fim</Data></Cell>
    <Cell ss:Index="5"><Data ss:Type="String">Tactical Operation name</Data></Cell>
