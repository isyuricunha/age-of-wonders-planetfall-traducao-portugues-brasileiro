    <Cell><Data ss:Type="String">Adversidade</Data></Cell>
    <Cell ss:Index="5"><Data ss:Type="String">Adversity as in overwhelming odds, a negative trait</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell><Data ss:Type="String">ADVERSITY</Data></Cell>
    <Cell><Data ss:Type="String">DESCRIPTION</Data></Cell>
