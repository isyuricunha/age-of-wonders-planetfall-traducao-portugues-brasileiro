  <Table ss:ExpandedColumnCount="6" ss:ExpandedRowCount="59" x:FullColumns="1" x:FullRows="1" ss:StyleID="s62" ss:DefaultColumnWidth="292.5" ss:DefaultRowHeight="15">
   <Column ss:StyleID="s62" ss:Width="90.75"/>
   <Column ss:StyleID="s62" ss:Width="145.5"/>
   <Column ss:StyleID="s62" ss:AutoFitWidth="0" ss:Width="655.5"/>
