    <Cell ss:StyleID="s63"><Data ss:Type="String">Cepa Ômega</Data></Cell>
    <Cell ss:StyleID="s63"/>
    <Cell ss:StyleID="s63"/>
    <Cell ss:StyleID="s64"/>
    <Cell ss:StyleID="s63"/>
