    <Cell ss:StyleID="s63"><Data ss:Type="String">TECH_TREE</Data></Cell>
    <Cell ss:StyleID="s63"><Data ss:Type="String">SKILLGROUP_1_TACTICAL_NAME</Data></Cell>
    <Cell ss:StyleID="s63"><Data ss:Type="String">Início da Cepa Alfa</Data></Cell>
    <Cell ss:StyleID="s63"/>
    <Cell ss:StyleID="s63"/>
    <Cell ss:StyleID="s65"/>
