   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:StyleID="s63"><Data ss:Type="String">TECH_TREE</Data></Cell>
    <Cell ss:StyleID="s63"><Data ss:Type="String">SKILLGROUP_6_FORCES_NAME</Data></Cell>
    <Cell ss:StyleID="s63"><Data ss:Type="String">Mutação do Senhor da Praga</Data></Cell>
    <Cell ss:StyleID="s63"/>
    <Cell ss:StyleID="s63"/>
