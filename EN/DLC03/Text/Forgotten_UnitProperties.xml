   <Row ss:AutoFitHeight="0">
    <Cell><Data ss:Type="String">ENTROPIC_TRANSPOSITIONER</Data></Cell>
    <Cell><Data ss:Type="String">TEMPLATE_NAME_1</Data></Cell>
    <Cell><Data ss:Type="String">Circumventing</Data></Cell>
