   <Row ss:AutoFitHeight="0" ss:Height="30">
    <Cell><Data ss:Type="String">ATROPHY</Data></Cell>
    <Cell><Data ss:Type="String">DESCRIPTION</Data></Cell>
    <Cell><Data ss:Type="String">This unit deals {damageReduction} less damage per stack.[br/]Stacks up to {stackcount} times.</Data></Cell>
   </Row>
