   <Row ss:AutoFitHeight="0">
    <Cell><Data ss:Type="String">AUTONOM_BUTTON</Data></Cell>
    <Cell><Data ss:Type="String">RELATION_WAR_PEACEKEEPER</Data></Cell>
    <Cell><Data ss:Type="String">We will relieve this planet of the Autonom.</Data></Cell>
