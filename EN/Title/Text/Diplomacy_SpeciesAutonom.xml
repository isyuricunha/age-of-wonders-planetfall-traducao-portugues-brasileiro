   <Row ss:AutoFitHeight="0">
    <Cell><Data ss:Type="String">AUTONOM_BUTTON</Data></Cell>
    <Cell><Data ss:Type="String">ATTACK_PEACEKEEPER</Data></Cell>
    <Cell><Data ss:Type="String">We request to communicate with your Network Controller.</Data></Cell>
