   <Row ss:AutoFitHeight="0">
    <Cell><Data ss:Type="String">AUTONOM_BUTTON</Data></Cell>
    <Cell ss:StyleID="s69"><Data ss:Type="String">CALL_TO_WAR_PEACEKEEPER</Data></Cell>
