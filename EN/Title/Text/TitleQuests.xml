   <Row ss:AutoFitHeight="0">
    <Cell><Data ss:Type="String">THE_WARRIOR</Data></Cell>
    <Cell><Data ss:Type="String">NAME</Data></Cell>
    <Cell><Data ss:Type="String">The Warmonger</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="45">
    <Cell><Data ss:Type="String">THE_WARRIOR</Data></Cell>
    <Cell><Data ss:Type="String">FLUFF_TEXT</Data></Cell>
    <Cell><Data ss:Type="String">Others have come to this world to steal its treasures and horde its secrets. This must not stand! Send forth your armies to punish these thieves and claim what is rightfully yours!</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell><Data ss:Type="String">THE_WARRIOR</Data></Cell>
    <Cell><Data ss:Type="String">PREREQUISUITE</Data></Cell>
    <Cell><Data ss:Type="String">Meet another player</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell><Data ss:Type="String">THE_WARRIOR</Data></Cell>
    <Cell><Data ss:Type="String">TASK_1</Data></Cell>
