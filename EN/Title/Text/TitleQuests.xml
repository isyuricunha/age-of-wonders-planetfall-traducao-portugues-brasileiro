    <Cell><Data ss:Type="String">TASK_3</Data></Cell>
    <Cell><Data ss:Type="String">Found a second colony</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell><Data ss:Type="String">THE_BUILDER</Data></Cell>
