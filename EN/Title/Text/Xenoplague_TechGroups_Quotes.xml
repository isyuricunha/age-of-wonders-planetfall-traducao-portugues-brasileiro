    <Cell ss:StyleID="s63"><Data ss:Type="String">Mutation: Destroyer</Data></Cell>
    <Cell ss:StyleID="s63"/>
    <Cell ss:StyleID="s63"/>
    <Cell ss:StyleID="s64"/>
    <Cell ss:StyleID="s63"/>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:StyleID="s63"><Data ss:Type="String">TECH_TREE</Data></Cell>
    <Cell ss:StyleID="s63"><Data ss:Type="String">SKILLGROUP_4_FORCES_SUBSKILL_2</Data></Cell>
    <Cell ss:StyleID="s63"><Data ss:Type="String">Equipment: Acrimonious</Data></Cell>
